{
	"imports": {
		"shed": "https://ghuc.cc/worker-tools/shed/index.ts"
	},
	"compilerOptions": {
		"strict": false,
		"implicitAny": true
	},
	"lint": {
		"rules": {
			"exclude": ["no-explicit-any", "no-unsafe-any"]
		}
	},
	"fmt": {
		"useTabs": true,
		"semiColons": false
	},
	"tasks": {
		"dev": "deno run --watch -A server.ts"
	},
	"unstable": ["kv", "temporal"]
}
